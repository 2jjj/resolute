    <div class="option" id="list-universe">
        <div class="options">
            <ul>
                <li><a href="/">Inicial</a></li>
                <!-- <li><a href="https://painel.resolutebot.xyz/">Dashboard</a></li> -->
                <li><a href="/commands">Comandos</a></li>
                <li><a href="/discord">Suporte</a></li>
            </ul>
        </div>
    </div>
    <header>
        <div class="navbar">
            <div class="logomark">
                <div class="avatar"></div>
                <div class="username">Resolute</div>
            </div>
            <div class="options">
                <ul>
                    <li><a href="/">Inicial</a></li>
                    <!--  <li><a href="https://painel.resolutebot.xyz/">Dashboard</a></li> -->
